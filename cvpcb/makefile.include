EXTRALIBS = ../common/common.a ../bitmaps/libbitmaps.a\
    ../polygon/lib_polygon.a\
    ../polygon/kbool/src/libkbool.a


LIBVIEWER3D = ../3d-viewer/3d-viewer.a

