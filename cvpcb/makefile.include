			tool_cvpcb.o \
			writenetlistpcbnew.o\
			genequiv.o \
			ioascii.o \
			menucfg.o \
			cfg.o\
            listlib.o \
			autosel.o \
			setvisu.o\
			dialog_display_options.o\
			zoom.o \
			tracemod.o \
