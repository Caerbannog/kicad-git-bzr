			class_cvpcb.o\
			class_zone.o\
			class_zone_setting.o\
            class_board_connected_item.o\
            memoire.o \
			cvframe.o\
			listboxes.o\
			drawframe.o\
			class_pcb_text.o\
			class_cotation.o\
			class_mire.o\
			displayframe.o\
			drawpanel.o \
			init.o\
			readschematicnetlist.o\
			loadcmp.o\
